    "transport-config": {
      "protocol-binding": {
        "protocol": "filesystem",
        "baseDir": "target/packages",
        "createIfMissing": "true",
        "overwrite": "true"
      }
    },
    "deposit-config": {
      "funder-mapping": {
        "nih": "",
        "ahrq": "",
        "aspr": "",
        "cdc": "",
        "epa": "",
        "fda": "",
        "hhmi": "",
        "nasa": "",
        "nist": "",
        "va": ""
      }
    }
  },
  "BagIt": {
    "assembler": {
      "specification": "simple",
      "beanName": "simpleAssembler",
      "options": {
        "archive": "ZIP",
        "compression": "NONE",
