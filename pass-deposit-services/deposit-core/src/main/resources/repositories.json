{
  "JScholarship": {
    "assembler": {
      "specification": "simple",
      "beanName": "simpleAssembler",
      "options": {
        "archive": "ZIP",
        "compression": "NONE",
        "algorithms": [
          "sha512"
        ]
      }
    },
    "transport-config": {
      "protocol-binding": {
        "protocol": "filesystem",
        "baseDir": "target/packages",
        "createIfMissing": "true",
        "overwrite": "true"
      }
    }
  },
  "PubMed Central": {
    "assembler": {
      "specification": "simple",
      "beanName": "simpleAssembler",
      "options": {
